
function binarySearch(li List<String>, item String) ->  Bool 
  var mid = li.len() div 2
  var value = li[mid]
  var result = false
  if not li.isEmpty() then  
     if item == value then
        result = true
     else if item < value then
        result = binarySearch(li[0..mid], item) 
     else 
        result = binarySearch(li[mid+1..], item)
     end if
  end if
  return result
end function
            

